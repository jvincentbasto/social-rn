{
  "name": "socialrn",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start-clean": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "npm-check-minor": "npx npm-check-updates -u --target minor",
    "android:bundle": "npx react-native bundle --platform android --dev false --entry-file node_modules/expo-router/entry.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "expo:doctor": "npx expo-doctor",
    "expo:pb": "npx expo prebuild",
    "expo:pb-clean": "npx expo prebuild --clean",
    "expo:c-android-w": "cd android && gradlew.bat clean -x externalNativeBuildCleanRelease",
    "expo:c-android-ml": "cd android && ./gradlew clean",
    "expo:c-ios": "cd ios && xcodebuild clean",
    "expo:bd-android-w": "cd android && gradlew.bat assembleDebug",
    "expo:bd-android-ml": "cd android && ./gradlew assembleDebug",
    "expo:bd-ios": "cd ios && xcodebuild",
    "expo:br-android-w": "cd android && gradlew.bat assembleRelease",
    "expo:br-android-ml": "cd android && ./gradlew assembleRelease",
    "expo:br-ios": "cd ios && xcodebuild",
    "eas:me": "npx eas whoami",
    "eas:login": "npx eas login",
    "eas:rb-prod": "npx eas build --profile production",
    "eas:rb-dev": "npx eas build --profile development",
    "eas:rb-devexpo": "npx eas build --profile devexpo",
    "eas:lb": "node -e \"require('child_process').execSync('npm run eas:rb-' + process.argv[1] + ' -- --local',{stdio:'inherit'})\"",
    "eas:submit": "npx eas submit"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.14.24",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.17",
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "expo": "~53.0.20",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-secure-store": "^14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.15.3",
    "react-native-web": "~0.21.1",
    "react-native-webview": "13.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@react-native-community/cli": "^20.0.1",
    "@types/react": "~19.1.11",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.34.0",
    "eslint-config-expo": "~9.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}